{
  "name": "daks-svelte",
  "version": "0.0.19",
  "description": "DVA svelte package",
  "repository": {
    "type": "git",
    "url": "https://github.com/diaksid/daks-svelte"
  },
  "author": "daks.dev",
  "homepage": "https://pkg.daks.dev/",
  "private": false,
  "scripts": {
    "clean": "rimraf .svelte-kit && rimraf build && rimraf package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev --port=3000",
    "build": "vite build",
    "preview": "vite preview --port=3000",
    "package": "svelte-kit sync && svelte-package",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "test": "playwright test",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
 "devDependencies": {
    "@iconify/svelte": "*",
    "@playwright/test": "*",
    "@sveltejs/adapter-node": "*",
    "@sveltejs/adapter-static": "*",
    "@sveltejs/kit": "*",
    "@sveltejs/package": "*",
    "@types/sharp": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "autoprefixer": "*",
    "cssnano": "*",
    "cssnano-preset-advanced": "*",
    "dotenv": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-svelte3": "*",
    "iconify-icon": "*",
    "postcss": "*",
    "postcss-import": "*",
    "postcss-load-config": "*",
    "prettier": "*",
    "prettier-plugin-svelte": "*",
    "rimraf": "*",
    "sharp": "*",
    "svelte-check": "*",
    "tailwindcss": "*",
    "tslib": "*",
    "typescript": "*",
    "vite": "*",
    "vitest": "*"
  },
  "peerDependencies": {
    "svelte": "*",
    "svelte-heros-v2": "*"
  },
  "exports": {
    "./open-sans": {
      "import": "./assets/fonts/open-sans/cyrillic.css"
    },
    "./open-sans/variable": {
      "import": "./assets/fonts/open-sans/variable.css"
    },
    "./tailwindcss": {
      "import": "./assets/tailwindcss/index.css"
    }
  },
 "type": "module"
}